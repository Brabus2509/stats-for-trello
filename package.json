{
  "name": "trello-stats",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015",
            "react"
          ]
        }
      ]
    ]
  },
  "scripts": {
    "css": "stylus < style.styl > assets/bundle.css",
    "js": "browserify app.js > assets/bundle.js",
    "build": "npm run js && npm run css",
    "watch": "find . -maxdepth 2 -type f -printf '%p\n' | grep -e '^\\.\\/\\w' | grep '.js' | entr fish -c 'npm run build; and echo done!'"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "immupdate": "^0.1.3",
    "react": "^0.14.3",
    "react-dom": "^0.14.3",
    "react-redux": "^4.0.3",
    "reactabular": "^0.9.2",
    "redux": "^3.0.5",
    "redux-thunk": "^1.0.2",
    "trello-browser": "0.0.5"
  },
  "devDependencies": {
    "babel-core": "^6.3.21",
    "babel-preset-es2015": "^6.3.13",
    "babel-preset-react": "^6.3.13",
    "babelify": "^7.2.0",
    "browserify": "^12.0.1",
    "stylus": "^0.53.0"
  }
}
